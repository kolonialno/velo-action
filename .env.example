INPUT_VELO_ARTIFACT_BUCKET_SECRET=velo_action_artifacts_bucket_name
INPUT_PROJECT=example-deploy-project
INPUT_SERVICE_ACCOUNT_KEY=None

# GitHub settings
GITHUB_WORKSPACE=../example-deploy-project
GITHUB_SHA=57c70505767e62275b467ccf57bf61c3b6441871
GITHUB_REF_NAME=exampleRef123
