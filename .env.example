# Trigger release
INPUT_CREATE_RELEASE=True

# Trigger deploy
INPUT_DEPLOY_TO_ENVIRONMENTS=dev
#INPUT_TENANTS=fc:ber1,fc:hel1
INPUT_WAIT_FOR_SUCCESS_SECONDS=600

# Configuration
